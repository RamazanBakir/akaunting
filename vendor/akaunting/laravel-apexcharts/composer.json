{
    "name": "akaunting/laravel-apexcharts",
    "description": "ApexCharts package for Laravel",
    "keywords": [
        "laravel",
        "line",
        "bar",
        "pie",
        "donut",
        "charts",
        "apexcharts"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cüneyt Şentürk",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=8.0",
        "balping/json-raw-encoder": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "orchestra/testbench": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Apexcharts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\Apexcharts\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Apexcharts\\Provider"
            ],
            "aliases": {
                "Apexcharts": "Akaunting\\Apexcharts\\Facade"
            }
        }
    }
}
